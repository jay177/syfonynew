{% extends 'base.html.twig' %}

{% block title %}Remontées{% endblock %}

{% block body %}
    <h1>Remontées</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ remont_e.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ remont_e.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ remont_e.type }}</td>
            </tr>
            <tr>
                <th>Open</th>
                <td>{{ remont_e.open ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Horaire</th>
                <td>{{ remont_e.horaire }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ remont_e.message }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_remont_es_index') }}">back to list</a>

    <a href="{{ path('app_remont_es_edit', {'id': remont_e.id}) }}">edit</a>

    {{ include('remontées/_delete_form.html.twig') }}
{% endblock %}
